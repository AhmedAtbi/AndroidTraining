<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidtrainigtaskproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_task_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_task_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidTrainigTaskProject">
        <activity android:name=".UpdateTaskActivity"></activity>
        <activity android:name=".AddActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AndroidTrainigTaskProject.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>